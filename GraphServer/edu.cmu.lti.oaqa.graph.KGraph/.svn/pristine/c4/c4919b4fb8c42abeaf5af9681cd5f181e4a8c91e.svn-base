package edu.cmu.lti.oaqa.graph.canonical;

public class CanonicalChunk {
	private Verb verb;
	private String noun;
	
	public CanonicalChunk(Verb verb, String noun){
		this.verb=verb;
		this.noun=noun;
	}
	
	public Verb getVerb(){
		return this.verb;
	}
	
	public String getNoun(){
		return this.noun;
	}
	
	@Override
	public String toString(){
		return this.verb.toString()+":"+this.noun;
	}
	
}
