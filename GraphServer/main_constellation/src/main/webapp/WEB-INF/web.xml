	version="3.0">

	<servlet>
		<display-name>HTTPHandler</display-name>
		<servlet-name>HTTPHandler</servlet-name>
		<servlet-class>edu.cmu.lti.oaqa.graph.constellation.HTTPHandler</servlet-class>
